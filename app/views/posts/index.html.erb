<div class="section">
  <table>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
    <% @posts.each do |post| %>
      <tr>
        <td><h5><%= link_to post.title, post_path(post.id) %></h5></td>
        <td><p><%= post.content %></p></td>
        <td><%= image_tag post.thumbnail %></td>
      </tr>
    <% end %>
  </table>
</div>
